{
  "name": "cozy-konnector-mgen",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konnectors/cozy-konnector-mgen.git"
  },
  "keywords": [],
  "author": "",
  "license": "AGPL-3.0",
  "main": "src/index.js",
  "renovate": {
    "extends": [
      "config:base"
    ],
    "patch": {
      "automerge": true
    }
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "jest": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ]
  },
  "eslintIgnore": [
    "build"
  ],
  "scripts": {
    "start": "node src/index.js",
    "dev": "cozy-konnector-dev src/index.js",
    "shell": "cozy-konnector-shell src/index.js",
    "standalone": "cozy-konnector-standalone src/index.js",
    "standalone:record": "REPLAY=record cozy-konnector-standalone src/index.js",
    "standalone:replay": "REPLAY=replay cozy-konnector-standalone src/index.js",
    "build": "webpack",
    "format": "prettier --write '**/*.{js,json}'",
    "precommit": "eslint . && pretty-quick --staged",
    "lint": "eslint .",
    "deploy": "git-directory-deploy --directory build/ --branch build --repo=https://github.com/konnectors/cozy-konnector-mgen.git",
    "deploy:prod": "git-directory-deploy --directory build/ --branch latest --repo=https://github.com/konnectors/cozy-konnector-mgen.git"
  },
  "dependencies": {
    "cozy-konnector-libs": "3.3.0"
  },
  "devDependencies": {
    "copy-webpack-plugin": "4.4.3",
    "eslint": "4.18.2",
    "eslint-config-prettier": "2.9.0",
    "git-directory-deploy": "1.5.1",
    "husky": "0.14.3",
    "prettier": "1.10.2",
    "pretty-quick": "1.4.1",
    "webpack": "3.11.0"
  }
}
